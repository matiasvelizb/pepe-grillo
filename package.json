{
  "name": "pepe-grillo",
  "version": "2.0.0",
  "description": "Discord bot that plays sounds from myinstants.com with guild-specific storage and interactive UI",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
